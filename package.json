  "devDependencies": {
    "esbuild": "^0.25.0",
    "esbuild-node-externals": "^1.18.0",
    "npm-dts": "^1.3.13",
    "typescript": "^5.7.0",
    "styxdefs": "^0.1.0"
  }
}
