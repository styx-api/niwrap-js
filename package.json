{
  "name": "niwrap",
  "version": "0.1.0",
  "description": "Neuroimaging JavaScript/TypeScript wrappers",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
